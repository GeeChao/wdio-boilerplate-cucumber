    "deepmerge": "^0.2.10",
    "node-uuid": "^1.4.7",
    "request": "^2.74.0",
    "wdio-cucumber-framework": "0.2.4",
    "wdio-dot-reporter": "0.0.6",
    "wdio-json-reporter": "0.0.4",
